   "source": [
    "def lazy_perceptron(\n",
    "    X_train, \n",
    "    y_train, \n",
    "    X_test, \n",
    "    y_test, \n",
    "    epochs=[20], \n",
    "    batch_size=[32], \n",
    "    nb_hidden_layers=[2],\n",
    "    nb_hidden_neurons=[10],\n",
    "    activation_function=[\"relu\"],\n",
    "    normalize=0,\n",
    "    standardize=0,\n",
    "):\n",
    "    nb_features = X_train.shape[1]\n",
    "    nb_classes = y_train.shape[1] \n",
    "    results = {}\n",
    "    \n",
    "    tab_X_train = [X_train]\n",
    "    tab_X_test = [X_test]\n",
    "    \n",
    "    # Normalize\n",
    "    if normalize > 0:\n",
    "        tab_X_train = [X_train_]\n",
    "    # Standardize\n",
    "    \n",
    "    for e in epochs:\n",
    "        for bs in batch_size:\n",
    "            for nhl in nb_hidden_layers:\n",
    "                for nhn in nb_hidden_neurons:\n",
    "                    for af in activation_function:\n",
    "                        model = get_sequential_model(nb_features, \n",
    "                                                     nhn, \n",
    "                                                     nb_classes, \n",
    "                                                     nhl,\n",
    "                                                     af)\n",
    "                        model.build(input_shape=(1, nb_features))\n",
    "                        model.compile(optimizer=optimizer, loss=criterion, metrics= \"accuracy\")\n",
    "                        \n",
    "                        for i in range(0, len(tab_X_train)):\n",
    "                            \n",
    "                        # Train\n",
    "                        start_time = datetime.now()\n",
    "                        history = model.fit(X_train, y_train, epochs=e, batch_size=bs, verbose=0)\n",
    "                        duration_fit = (datetime.now() - start_time)\n",
    "                        \n",
    "                        # Test\n",
    "                        start_time = datetime.now()\n",
    "                        evaluation = model.evaluate(X_test, y_test, verbose=0)\n",
    "                        duration_evaluation = (datetime.now() - start_time)\n",
    "                        \n",
    "                        # Save Results\n",
    "                        results[f\"{e}/{bs}/{nhl}/{nhn}/{af}\"] = (history, evaluation, duration_fit, duration_evaluation)\n",
    "                        \n",
    "                        print(f\"{e}/{bs}/{nhl}/{nhn}/{af} : TRAIN : accuracy {history.history['accuracy'][-1]*100:.2f}% - loss {history.history['loss'][-1]:.4f} - duration: {duration_fit} - TEST accuracy {evaluation[1]*100:.2f}% - loss {evaluation[0]:.4f} - duration: {duration_evaluation}\")\n",
    "    return results\n",
    "                        \n",
    "                        "
   ]
  },
